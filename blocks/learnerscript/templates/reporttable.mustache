<div id="reportcontainer{{reportinstance}}">
    <div id="printablediv" class="p-4">
        <div id="map_{{reportinstance}}" class="row map_report_container">
            <div class="col-md-12">
                <div id="reportloading_{{reportinstance}}" style="display:none;">
                    <img src={{loading}} alt="loading" title="loading" />
                </div>
                <div class="table-responsive">
                    <table  width={{width}} summary='{{summary}}' cellpadding={{cellpadding}} cellspacing={{cellspacing}} class="{{class}} dependencyfilter" boxalign{{align}} id=reporttable_{{reportinstance}}>
                        <thead class="table_{{reporttype}}">
                            {{# tablehead}}
                            <th style="vertical-align:middle;white-space:normal;{{align}} {{size}} {{wrap}}" class="header c{{key}}" scope="col">
                            {{heading}}</th>
                            {{/ tablehead}}
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
{{# debugsql}}
<hr>
	<div id='debug' style='direction:ltr;text-align:left;'>
		{{reportsql}}
	</div>
</hr>
{{/ debugsql}}
{{> block_learnerscript/exportoptions }}
