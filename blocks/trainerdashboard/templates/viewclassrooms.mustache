{{#response}}
    {{^records}}
    {{#nodata}}
        <div class = 'w-100 pull-left alert alert-info'>{{nodata}}</div>
    {{/nodata}}
    {{/records}}
    {{#viewtypeCard}}     

                <table class="generaltable">
                    <thead>
                        <tr>
                            <th>{{#str}}classroomname,block_trainerdashboard{{/str}}</th>
                            <th>{{#str}}sessioncount,block_trainerdashboard{{/str}}</th>
                            <th>{{#str}}enrolled,block_trainerdashboard{{/str}}</th>
                            <th>{{#str}}completed,block_trainerdashboard{{/str}}</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#records}}
                        <tr>
                            <td><a href="{{cfgwwwroot}}">{{classroomname}}</a></td>
                            <td>{{sessioncount}}</td>
                            <td>{{enrolled}}</td>
                            <td>{{completed}}</td>
                        </tr>

                        {{/records}}
                    </tbody>
                </table>
    {{/viewtypeCard}}
{{/response}}
