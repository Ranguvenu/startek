    {{#response}}
        {{#viewtypeCard}}
        <div style="overflow: auto;">
            <table class="generaltable dataTable no-footer" id="all_skills_acquired" role="grid" style="width: 100%;">
                <thead>
                    <tr>
                        <th class="header c0 sorting_asc" style="text-align: left; width: 103px;" >
                            Skill
                        </th>
                        <th class="header c1 sorting" style="text-align: left; width: 125px;">
                            Level
                        </th>
                        <th class="header c2 sorting" style="text-align: left; width: 120px;" >
                            Course Name
                        </th>
                        <th class="header c4 lastcol sorting" style="text-align: left; width: 78px;">
                            Achieved On
                        </th>
                    </tr>
                </thead>
                <tbody>
                    {{#records}}
                        <tr>
                            <td  style="text-align: left;">{{skill_name}}</td>
                            <td  style="text-align: center;">{{levelname}}</td>
                            <td  style="text-align: left;">{{course_name}}</td>
                            <td  style="text-align: left;">{{achieved_on}}</td>
                        </tr>
                   {{/records}}
              </tbody>
        </table>
    </div>
    {{/viewtypeCard}}
{{/response}}
