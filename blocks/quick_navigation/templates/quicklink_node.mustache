{{#view_type}}
<div class="quick_nav_list card_view h-100 manage_{{pluginname}} {{space_count}}_of_three_columns d-flex bg-white flex-column p-0 align-items-center" >
    <div class="quick_course bg-primary w-100 d-flex align-items-center justify-content-between">
        
        <div class=' quick_nav content w-100 d-flex align-items-center justify-content-between'>
            <div class='quick_navigation_detail'>
                <div class='span_str'>{{{node_header_string}}}</div>
                
            </div>
         </div>
        <span class='anch_span'>
            {{# plugin_icon_class}}
                <i class="{{plugin_icon_class}}"></i>
            {{/ plugin_icon_class}}
    <!--         {{^ plugin_icon_class}}
                <span class='{{pluginname}}_icon_wrap'></span>
            {{/ plugin_icon_class}} -->
        </span>
        
        
    </div>
    <div class="quick_nav_links px-3 pt-3 pb-2 d-flex flex-column h-100 w-100 bg-white">
         <div class=" actions d-flex align-items-center justify-content-center">
            <div class='span_createlink d-flex align-items-center'>
                {{#create}}
                    <div class="create_element">{{{create_element}}}</div> <div class="vertical_divider"></div>
                {{/create}}
                {{# view}}
                    <a href="{{viewlink_url}}" class='viewlink ml-2' title= "{{viewlink_title}}">
                        {{#str}}view{{/str}}
                    </a>
                {{/ view}}
            </div>
        </div>
        {{# displaystats}}
            <!-- <div class="w-100 d-flex">
                {{# percentage}}
                <div class="progress mx-auto mt-3 mb-3">
                    <div class="progress-bar" role="progressbar" style="width: {{percentage}}%;" aria-valuenow="{{percentage}}" aria-valuemin="0" aria-valuemax="100">{{percentage}}%</div>
                </div>
                {{/ percentage}}
            </div> -->
            <div class="dashboard_count_list w-100 d-flex p-2 mt-auto">
                <div class="dashbaord_count_item"><span class="d-flex align-items-center">
                    <span class="d-block dashboard_count_string mr-1">{{#str}}total,block_quick_navigation{{/str}}</span><span class="dashboard_count_value">
                    {{# view}}
                    <a href="{{viewlink_url}}" class='viewlink' title= "{{viewlink_title}}">
                            {{count_total}}</a>
                     {{/ view}}</span></span>
                </div>
                <div class="vertical_divider"></div>
                <div class="dashbaord_count_item">
                    <span class="d-flex align-items-center">
                    {{# enroll_string}}
                    <span class="d-block dashboard_count_string mr-1">{{enroll_string}} </span><span class="dashboard_count_value">
                    {{/ enroll_string}}
                    {{^ enroll_string}}
                    <span class="d-block dashboard_count_string mr-1">{{#str}}active,block_quick_navigation{{/str}} </span><span class="dashboard_count_value">
                    {{/ enroll_string}}
                        {{# view}}
                        {{# count_activelink_url}}
                        <a href="{{count_activelink_url}}" class='viewlink' title= "{{count_activelink_url}}">{{count_active}}</a>
                        {{/ count_activelink_url}}
                        {{^ count_activelink_url}}
                        <a  title= "{{count_activelink_url}}">{{count_active}}</a>
                        {{/ count_activelink_url}}
                        {{/ view}}</span></span>
                </div>
                <div class="vertical_divider"></div>
                <div class="dashbaord_count_item"><span class="d-flex align-items-center">
                    <span class="d-block dashboard_count_string mr-1">{{inactive_string}} </span><span class="dashboard_count_value">
                    {{# view}}
                    {{# count_inactivelink_url}}
                        <a href="{{count_inactivelink_url}}" class='viewlink' title= "{{count_inactivelink_url}}">{{count_inactive}}</a>
                    {{/ count_inactivelink_url}}
                    {{^ count_inactivelink_url}}
                        <a title= "{{count_inactivelink_url}}">{{count_inactive}}</a>
                    {{/ count_inactivelink_url}}
                        {{/ view}}</span></span>
                </div>
            </div>
        {{/ displaystats}}
        <div class="additional_info_labels d-none my-3 w-100">
            <div class="lable_container">
                <span class="label">Total Enrolements :</span> <span class="count">10</span>
            </div>
            <div class="lable_container">
                <span class="label">Total Enrolements :</span> <span class="count">10</span>
            </div>
        </div>
    </div>
    
</div>
{{/view_type}}

{{^view_type}}
<div class="quick_nav_list manage_{{pluginname}} {{space_count}}_of_three_columns d-flex justify-content-between flex-column align-items-center" >
    <div class="quick_course d-flex align-items-center justify-content-between">
        <div class='anch_span'>
            {{# plugin_icon_class}}
                <i class="{{plugin_icon_class}}"></i>
            {{/ plugin_icon_class}}
    <!--         {{^ plugin_icon_class}}
                <span class='{{pluginname}}_icon_wrap'></span>
            {{/ plugin_icon_class}} -->
        </div>
        <div class=' quick_nav content w-100 d-flex align-items-center justify-content-between'>
            <div class='quick_navigation_detail w-100'>
                <div class='span_str'>{{{node_header_string}}}</div>
                <div class='span_createlink'>
                    {{#create}}
                        {{{create_element}}} |
                    {{/create}}
                    {{# view}}
                    <a href="{{viewlink_url}}" class='viewlink' title= "{{viewlink_title}}">
                            {{#str}}view{{/str}}
                        </a>
                    {{/ view}}
                </div>
            </div>
         </div>
    </div>
    {{# displaystats}}
        <div class="w-100 d-flex">
            {{# percentage}}
            <div class="progress mx-auto mt-3 mb-3">
                <div class="progress-bar" role="progressbar" style="width: {{percentage}}%;" aria-valuenow="{{percentage}}" aria-valuemin="0" aria-valuemax="100">{{percentage}}%</div>
            </div>
            {{/ percentage}}
        </div>
        <ul class="dashboard_count_list w-100 p-2 mt-auto">
            <li class="dashbaord_count_item"><div class="">
                <span class="d-block dashboard_count_string">{{#str}}total,block_quick_navigation{{/str}}</span><span class="dashboard_count_value">
                {{# view}}
                <a href="{{viewlink_url}}" class='viewlink' title= "{{viewlink_title}}">
                        {{count_total}}</a>
                 {{/ view}}</span></div>
            </li>
            <li class="dashbaord_count_item"><div class="">
                {{# enroll_string}}
                <span class="d-block dashboard_count_string">{{enroll_string}}</span><span class="dashboard_count_value">
                {{/ enroll_string}}
                {{^ enroll_string}}
                <span class="d-block dashboard_count_string">{{#str}}active,block_quick_navigation{{/str}}</span><span class="dashboard_count_value">
                {{/ enroll_string}}
                    {{# view}}
                    {{# count_activelink_url}}
                    <a href="{{count_activelink_url}}" class='viewlink' title= "{{count_activelink_url}}">{{count_active}}</a>
                    {{/ count_activelink_url}}
                    {{^ count_activelink_url}}
                    <a  title= "{{count_activelink_url}}">{{count_active}}</a>
                    {{/ count_activelink_url}}
                    {{/ view}}</span></span></div>
            </li>
            <li class="dashbaord_count_item"><div class="">
                <span class="d-block dashboard_count_string">{{inactive_string}}</span><span class="dashboard_count_value">
                {{# view}}
                {{# count_inactivelink_url}}
                    <a href="{{count_inactivelink_url}}" class='viewlink' title= "{{count_inactivelink_url}}">{{count_inactive}}</a>
                {{/ count_inactivelink_url}}
                {{^ count_inactivelink_url}}
                    <a title= "{{count_inactivelink_url}}">{{count_inactive}}</a>
                {{/ count_inactivelink_url}}
                    {{/ view}}</span></div>
            </li>
        </ul>
    {{/ displaystats}}
</div>




{{/view_type}}
