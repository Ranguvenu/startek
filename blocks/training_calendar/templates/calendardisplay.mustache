{{!
    @template block_training_calendar/allevents
    displaying calendar days
}}
    

    <ul class="calender_clr_list pull-right">
        <li class="caleder_clr_list_item evalaluation">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}feedbacks,block_training_calendar{{/ str}}  </span></span>
        </li>
        <li class="caleder_clr_list_item classroom">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}classrooms,block_training_calendar{{/ str}}</span></span>
        </li>
        <li class="caleder_clr_list_item program">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}programs,block_training_calendar{{/ str}}</span></span>
        </li>
        <li class="caleder_clr_list_item certification">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}certifications,block_training_calendar{{/ str}}</span></span>
        </li>
        <li class="caleder_clr_list_item onlinetests">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}onlineexams,block_training_calendar{{/ str}}</span></span>
        </li>
        <li class="caleder_clr_list_item mod">
            <span class=""><span class="pull-left mr-2 calendar_spot_clr"></span><span class="pull-left calendar_clr">
			{{# str}}courseactivities,block_training_calendar{{/ str}}</span></span>
        </li>
    </ul>

    <lable for="type">{{# str}}type,block_training_calendar{{/ str}}</lable>
    <select name="type" class="target custom-select ml-15" id="id_eventtype" >
    <option data-ignore="" value="all" selected="">{{# str}}all,block_training_calendar{{/ str}}</option>
    {{#types}}
        <option value={{key}}>{{value}}</option>
    {{/types}}
    </select>
    <div id='calendar' class="mt-3"></div>
        <p id='eventInfo'></p>
    <div id='pop_desc'></div>
                


