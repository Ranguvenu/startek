<!-- <div><h3>{{#str}}suggestedmodules, local_tags{{/str}}</h3></div>-->
<table id="feedbackreminderstable" class="generaltable feedbacksreminderpopup">
    <thead>
        <tr>
            <th>{{#str}}modname, local_tags{{/str}}</th>
            <th>{{#str}}modtype, local_tags{{/str}}</th>
            <th>{{#str}}action, local_tags{{/str}}</th>
        </tr>
    </thead>
    <tbody>
        {{# popdata}}
            <tr>
                <td class="">{{name}}</td>
                <td class="">{{modtype}}</td>
                <td>{{{ link }}}</td>
            </tr>
        {{/ popdata}}
    </tbody>
</table>
{{#viewmore}}
<button class="btn btn-primary">{{{ viewmorelink }}}</button>    
{{/ viewmore}}