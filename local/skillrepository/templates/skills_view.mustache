{{#response}}
    {{#viewtypeCard}}
        <table class="generaltable dataTable no-footer" id="skill_repository" role="grid" aria-describedby="skill_repository">
            <thead>
                <tr role="row">
                    
                    <th class="header text-left" tabindex="0">{{# str}} skill_name, local_skillrepository{{/ str}}</th>
                    <!-- <th class="header text-left" tabindex="0">{{# str}} skill_category, local_skillrepository{{/ str}}</th> -->
                    <th class="header text-center" tabindex="0">{{# str}} achieved_users, local_skillrepository{{/ str}}</th>
                    <th class="header text-center" tabindex="0">{{# str}} shortname, local_skillrepository{{/ str}}</th>
                    {{# is_admin}}
                        <th class="header text-left" tabindex="0">{{# str}} open_costcenterid, local_costcenter{{/ str}}
                        </th>
                    {{/ is_admin}}
<!--                     {{# advance}}
                    <th class="header text-center" tabindex="0">Levels</th>
                    {{/ advance}} -->
                    <th class="header text-center" tabindex="0">{{# str}} action, local_skillrepository{{/ str}}</th>
                </tr>
            </thead>
            <tbody>
                {{#records}}
                    <tr>                       
                        <td class="text-left">
                            <a href="../skillrepository/skillinfo.php?id={{skill_id}}">{{{skilname}}}</a>
                        </td>
                        <!-- <td class="text-left">{{skill_catname}}</td> -->
                        <td class="text-center">{{achieved_users}}</td>
                        <td class="text-center">{{shortname}}</td>
                        {{# is_admin}}
                        <td class="text-left">{{organisationname}}</td>
                        {{/ is_admin}}
<!--                         {{# advance}}
                        <td>
                            <a href="javascript:void(0)" onclick="(function(e){ require('local_skillrepository/newassignlevel').displaySkillLevels({ skillid:{{skill_id}}, skillname:'{{skilname}}' ,costcenterid: {{organisationid}},contextid:1 }) })(event)">{{levelsCount}}</a>
                        </td>
                        {{/ advance}} -->
                        <td class="text-center">
                            <a href="javascript:void(0)" data-action="createrepositorymodal" class="iconsmall" data-value="{{skill_id}}" onclick="(function(e){ require('local_skillrepository/newrepository').init({selector:'createrepositorymodal', contextid:1, repositoryid:{{skill_id}}}) })(event)" style="cursor:pointer" title="">
                                <i class="fa fa-pencil fa-fw" title="{{# str }} edit_title, local_skillrepository {{/ str }}"></i>
                            </a>
                        {{^skillmaped}}
                            <a href="javascript:void(0)" title="{{# str}}delete{{/ str}}" onclick="(function(e){ require('local_skillrepository/newrepository').deleteskill({selector: 'deleteskill' ,skillid:{{skill_id}},contextid:1, skillname:'{{skilname}}'}) })(event)">
                                <i class="fa fa-trash fa-fw" aria-hidden="true" title="{{# str }} delete_title, local_skillrepository {{/ str }}" aria-label="Delete"></i>
                            </a>
                        {{/skillmaped}}
                        {{#skillmaped}}
                            <a href="javascript:void(0)" title="{{# str}}delete{{/ str}}" onclick="(function(e){ require('local_skillrepository/newrepository').nodeleteskill({selector: 'deleteskill' ,skillid:{{skill_id}},contextid:1, skillname:'{{skilname}}'}) })(event)">
                                <i class="fa fa-trash fa-fw" aria-hidden="true" title="{{# str }} delete_title, local_skillrepository {{/ str }}" aria-label="Delete"></i>
                            </a>
                        {{/skillmaped}}
                        
                {{/records}}
            </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}



