{{#competency}}
<div class="competencyskill_view">
    <div class="description border rounded p-3 ">
        <h6 class="desc_head d-flex align-items-center"><span class="desc_icon mr-1"></span>Description</h6>
        <p class="desc text-muted pl-4">{{{description}}}</p></div>
    <div class="row mt-3">
        <div class="col-md-3">
            <div class="card">
                <div class="skill_card d-flex p-3">
                    <div class="skill_icon"></div>
                    <div class="card_body ml-3">
                        <h6 class="card-title d-flex flex-column mb-0"><div class="text-muted">{{# str }} skills,local_skillrepository {{/ str}}</div>
                            <div class="skill_count">{{skillcount}}</div>
                        </h6>
                    </div>
                </div>
            </div>
        </div>
    </div>
<!--  <div class="card" style="width: 18rem;">
   <div class="card-body">
     <h5 class="card-title">Achieved User : 1</h5>
   </div>
 </div> -->
 <!-- <div class="d-flex justify-content-end">{{{assignskill}}}</div> -->
<div class="competency_container row">
    <div class="col-12 col-xl-12 domain_container">
        <div class="details_comp_level_name">
            <div class="competency_level_names content_left d-flex flex-row flex-wrap justify-content-start align-items-center">
            </div>
            <div class="panel panel-default border rounded mb-3">
                <div class="panel-heading p-3 bg-white text-dark d-flex justify-content-between align-items-center border-bottom" data-toggle="collapse" data-target="#collapse-bodyskillid" aria-expanded="false" aria-controls="competency_level_skills">
                    <h6 class="mb-0">{{# str }} skills,local_skillrepository {{/ str}}</h6>                            
                    <div class="d-flex align-items-center">
                        <div class="d-flex justify-content-end">{{{assignskill}}}</div>
                        <span class="collapse_icon ml-3"><i class="fa fa-angle-down"></i></span>
                    </div>
                </div>
                <div class="panel-body mx-3 p-2 collapse" id="collapse-bodyskillid" >
                {{#competency_level_skills}}
                    <div class="panel panel-default border rounded mb-3">
                        <div class="panel-heading p-3 bg-white text-dark d-flex justify-content-between align-items-center border-bottom" data-toggle="collapse" data-target="#collapse-body{{comp_level_skill_id}}" aria-expanded="false" aria-controls="competency_level_skills">
                            <h6 class="mb-0">{{comp_level_skill_name}}</h6>
                            <div class="d-flex align-items-center">
                                <div class="action_icons d-flex align-items-center">
                                    {{{comp_level_skill_assignlevel}}}
                                    <a href="javascript:void(0)" title="{{# str }} delete,local_skillrepository {{/ str}}" class="removeLevelSkill" data-costcenterid="{{costcenterid}}" data-competencyid="{{id}}" data-levelid="{{comp_level_id}}" data-skillid="{{comp_level_skill_id}}" data-levelname="{{comp_level_name}}" data-skillname="{{comp_level_skill_name}}">
                                                <i class="fa fa-trash"></i>
                                    </a>
                                </div>
                                <span class="collapse_icon ml-3"><i class="fa fa-angle-down"></i></span>
                            </div>
                                    
                        </div>
                        <div class="panel-body mx-3 p-2 collapse" id="collapse-body{{comp_level_skill_id}}" >
                            <div class="panel panel-default">
                                {{# lable}}
                                    <h6>{{lable}} :</h6>
                                {{/ lable}}
                                {{#skill_levels}}
                                    <div class="competency_level_skill_levels content_left d-flex flex-row flex-wrap justify-content-start align-items-center px-3 py-2">
                                        <span class="competency_level_skills_name d-flex flex-row flex-wrap justify-content-start align-items-center col-md-12 col-lg-4 p-0 pr-1"><b>{{ comp_level_skill_level_name }}</b></span>
                                        <span class="competency_level_skills_name d-flex flex-row flex-wrap justify-content-center align-items-center col-md-12 col-lg-4 p-0 pr-1">{{# str }} coursepopup,local_skillrepository {{/ str}} : &nbsp <b>{{{skill_level_courses_count}}}</b></span>
                                        <span class="competency_level_skill_assignlevel d-flex justify-content-end align-items-center col-md-12 col-lg-4 p-0 pr-1 pl-lg-1">{{{ comp_level_skill_assignskill_level_course }}}
                                        {{{skill_level_remove}}}</span>
                                    </div>
                                {{/skill_levels}}

                            </div>
                        </div>

                    </div>
                {{/competency_level_skills}}
                </div>
            </div>
           </div>
        </div>
    </div>
</div>
{{/competency}}
