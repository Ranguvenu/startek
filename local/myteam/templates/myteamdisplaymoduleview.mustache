{{#response}}
    {{#viewtypeCard}}
                <table id ="displaymodulewisedata" class="generaltable">
                    <thead>
                        <tr>
                            <th>Name</th>

                            <th>{{# str}}enrolldate, local_myteam{{/ str}}</th>

                            <th>{{# str}}status, local_myteam{{/ str}}</th>

                            <th>{{# str}}completedon, local_myteam{{/ str}}</th>

                            {{# evaluation}}
                                <th>{{# str}}action, local_myteam{{/ str}}</th>
                            {{/ evaluation}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#records}}
                        <tr>
                            <td>{{name}}</td>

                            <td>{{enrolldate}}</td>

                            <td>{{status}}</td>

                            <td>{{completiondate}}</td>

                            {{# evaluation}}
                                <td>{{{evaluation_button}}}</td>
                            {{/ evaluation}}
                        </tr>
                        {{/records}}
                    </tbody>
                </table>
               
    {{/viewtypeCard}}
{{/response}}


