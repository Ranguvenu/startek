{{#response}}
    {{#viewtypeCard}}
        <table id ="displaysyncerrors" class="generaltable">
        <thead>
            <tr>
                <th>{{#str}}employee_id, local_users{{/str}}</th>
                <th>{{#str}}email{{/str}}</th>
                <th>{{#str}}error_with, local_users{{/str}}</th>
                <th>{{#str}}description{{/str}}</th>
                <th>{{#str}}uploaded_by, local_users{{/str}}</th>
                <th>{{#str}}uploaded_on, local_users{{/str}}</th>
            </tr>
        </thead>
        <tbody>
            {{#records}}
                <tr>
                    <td>{{idnumber}}</td>
                    <td>{{email}}</td>
                    <td>{{{mandatoryfields}}}</td>
                    
                    <td>{{{errors}}}</td>
                    <td>{{modifiedby}}</td>
                    <td>{{createddate}}</td>
                </tr>
            {{/records}}
        </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}
