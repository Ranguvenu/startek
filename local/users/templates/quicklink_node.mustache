<div class="quick_nav_list manage_users two_of_three_columns" >
    <span class='anch_span'>
        <i class='fa fa-user-plus' aria-hidden='true'></i>
    </span>

    <div class='w-100 pull-left'>
        <div class='quick_navigation_detail'>
            <div class='span_str'>
                {{# str}}manage_br_users, local_users{{/ str}}
            </div>
            <span class='span_createlink'>
                {{#hascapablity}}
                     <a href='javascript:void(0);' class='quick_nav_link goto_local_users' data-action='createusermodal' title='{{# str}}createuser, local_users{{/str}}' class='course_extended_menu_itemlink' data-action='createusermodal' onclick ='(function(e){ require("local_users/newuser").init({selector:"createusermodal", context:{{contextid }}, userid:{{userid}}, form_status:0}) })(event)' >{{# str}}create{{/str}}
                    </a>
                {{/hascapablity}}

                {{#displayline}}
                    |
                {{/displayline}}

                <a href='{{config.wwwroot}}/local/users/index.php' class='viewlink' title='{{# str}}view_users, local_users{{/ str}}' >
                    {{# str}}view{{/ str}}
                </a>
            </span>
        </div>
    </div>

    <div class="w-100 pull-left">
        <div class="progress w-75 mx-auto my-5">
            <div class="progress-bar" role="progressbar" style="width: {{percent}}%;" aria-valuenow="{{percent}}" aria-valuemin="0" aria-valuemax="100">{{percent}}%</div>
        </div>
    </div>
    <ul class="dashboard_count_list w-100 pull-left p-15">
        <li class="dashbaord_count_item"><span class="">
            <span class="d-block dashboard_count_string">{{# str}}total, local_users{{/ str}}</span><span class="dashboard_count_value">{{countusers}}</span></span>
        </li>
        <li class="dashbaord_count_item"><span class="">
            <span class="d-block dashboard_count_string">{{# str}}active, local_users{{/ str}}</span><span class="dashboard_count_value">{{countactiveusers}}</span></span>
        </li>
        <li class="dashbaord_count_item"><span class="">
            <span class="d-block dashboard_count_string">{{# str}}inactive, local_users{{/ str}}</span><span class="dashboard_count_value">{{countinactiveusers}}</span></span>
        </li>
    </ul>

</div>