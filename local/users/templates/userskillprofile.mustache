{{#response}}
{{#viewtypeCard}}
<div class="Comcourses_tab_content" id="courses_tab_content">
    <table class="w-full generaltable Comcourses_table" id = profile_course_content>
        <thead class="hidden">
            <tr>
                <th></th><th></th>
            </tr>
        </thead>

            <tbody>
                {{#records}}
                <tr>
                    <td>{{{ comp_name }}}</td>
                    <td>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: {{percentage}}%;" aria-valuenow="{{percentage}}" aria-valuemin="0" aria-valuemax="100" title="{{skillnames}}">{{percentage}}%</div>
                        </div>
                    </td>
                </tr>
                {{/ records}}
            </tbody>
    </table>

    {{^ records }}
        <p class="w-full pull-left alert alert-info text-center">{{# str}}team_nodata, local_users {{/ str}}</p>
    {{/ records }}
</div>
{{/viewtypeCard}}
{{/response}}