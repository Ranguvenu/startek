{{#response}}
{{#viewtypeCard}}
<table id ="manage_feedbacks" class="generaltable">
    <thead>
        <tr>
            <th>{{#str}}evaluationname, local_evaluation{{/str}}</th>
            <th>{{#str}}type, local_evaluation{{/str}}</th>
       
            <th>{{#str}}enrolled, local_evaluation{{/str}}</th>
            <th>{{#str}}completed, local_evaluation{{/str}}</th>
         
             
            <th>{{# str}} schedule, local_evaluation {{/ str}} </th>
            <th>{{# str}} response, local_evaluation {{/ str}} </th>
            {{#has_cap}}
            <th>{{#str}}actions, local_evaluation{{/str}}</th>
            {{/has_cap}}
        </tr>
    </thead>
    <tbody>
        {{#records}}
        <tr class="{{^ status}} inactive{{/ status}}">
            <td>
              <span class="m-0 evalname font-weight-bold" title="{{ eval_name }}">{{{ evalname }}} </span>
            </td>            
        
            <td>{{ evaltype }}</td>
            {{# has_evalcap}}
            <td> {{{enrolled}}} </td>
            <td> {{{completed}}}</td>
            {{/ has_evalcap}}
            {{^ has_evalcap}}
            <td> {{enrolledon}} </td>
            <td> {{completedon}}</td>
            {{/ has_evalcap}}
            <td> 
              <p class="mb-1" title="{{# str}}schedule, local_evaluation{{/ str}}">
                 <span class="pull-left eval_schedule_label font-weight-bold">
                    <i class="fa fa-calendar pr-2" aria-hidden="true"></i>
                 </span>
                 <span style="font-size: 15px;color: #000;font-weight: 500;">{{ schedule }}</span>
              </p>
            </td>
        
            <td>
                 {{# current_feedback}}
                     {{^ completed}}
                         {{# actions}}
                             <a class="d-block take_evaluation evalactions" href="{{{previewurl}}}">{{previewstring}}</a>
                          {{/ actions}}
                    {{/ completed}}
                                
                    {{# completed}}
                        <a class="d-block take_evaluation evalactions" href="{{{completedurl}}}">{{# str}}viewresponse, local_evaluation{{/str}}</a>
                    {{/ completed}}
                {{/ current_feedback}}

                {{^ current_feedback}}
                    {{# closed_feedback}}
                        <a class="d-block take_evaluation evalactions" href="{{{completedurl}}}">{{# str}}closed, local_evaluation{{/ str}}</a>
                    {{/ closed_feedback}}
                    {{# not_yetstarted}}
                        <span class="take_evaluation evalactions">{{# str}}not_yet_started, local_evaluation{{/ str}}</span>
                    {{/ not_yetstarted}}
                {{/ current_feedback}}
                {{{ copyaction }}}
            </td>
            <td> 
                {{# actions}}
                       {{{edit_eval}}}
                       {{{eval_enrol}}}
                       {{{eval_hideshow}}}
                       {{{eval_analys}}}
                       {{{edit_template}}}
                       {{{edit_question}}}
                       {{{eval_delete}}}
                {{/ actions}}
              </td>
        </tr>
       {{/records}}
    </tbody>
</table>
{{/viewtypeCard}}
{{/response}}
