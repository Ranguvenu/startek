{{#response}}
    {{#viewtypeCard}}
        <table class="generaltable dataTable no-footer" id="skill_repository" role="grid" aria-describedby="skill_repository">
            <thead>
                <tr role="row">
                    <th class="header text-left" tabindex="0">
                        {{#parentcatid}}
                            {{# str}} performancefieldname, local_custom_matrix{{/ str}}
                        {{/parentcatid}}
                        {{^parentcatid}}
                            {{# str}} typefieldname, local_custom_matrix{{/ str}}
                        {{/parentcatid}}
                    </th>
                    <th class="header text-left" tabindex="0">
                        {{#parentcatid}}
                            {{# str}} performanceshortname , local_custom_matrix{{/ str}}
                        {{/parentcatid}}
                        {{^parentcatid}}
                            {{# str}} typeshortname, local_custom_matrix{{/ str}}
                        {{/parentcatid}}
                    </th>                  
                    <th class="header text-left" tabindex="0">{{# str}} organization, local_custom_matrix{{/ str}}</th>
                    {{^ parentcatid}}
                    <th class="header text-left" tabindex="0">{{# str}} fieldvalue, local_custom_matrix{{/ str}}</th>
                    {{/ parentcatid}}
                    
                    <th class="header text-center" tabindex="0">{{# str}} action, local_custom_matrix{{/ str}}</th>
                </tr>
            </thead>
            <tbody>
                {{#records}}
                    <tr>
                        {{^ childs}}
                        <td class="text-left" >{{custom_matrix_name}}</td>
                        {{/ childs}}
                        {{# childs}}
                        <td class="text-left" >{{custom_matrix_name}}</td>
                        {{/ childs}}
                        <td class="text-left">{{shortname}}</td>                       
                        <td class="text-left">{{organisationname}}</td>
                        {{^ childs}}
                        <td class="text-left"><a href="{{wwwroot}}?parentcatid={{custom_matrix_id}}">{{childcount}}</a></td>
                        {{/ childs}}
                        <td class="text-center">
                            <a href="javascript:void(0)" data-action="createrepositorymodal" class="iconsmall" data-value="{{custom_matrix_id}}" onclick="(function(e){ require('local_custom_matrix/newcustommatrix').init({selector:'createrepositorymodal', contextid:1, repositoryid:{{custom_matrix_id}}, parentcatid:{{childs}}}) })(event)" style="cursor:pointer" title="Edit">
                                <i class="fa fa-pencil fa-fw" title="{{# str }} edit_title, local_custom_matrix {{/ str }}"></i>
                            </a>
                            {{^ matrixexist}}
                            <a href="javascript:void(0)" title="{{# str}}delete{{/ str}}" onclick="(function(e){ require('local_custom_matrix/newcustommatrix').deletematrix({selector: 'deletecustommatrix', matrixid:{{custom_matrix_id}}, name: '{{custom_matrix_name}}' ,contextid:1}) })(event)">
                                <i class="fa fa-trash fa-fw" aria-hidden="true" title="{{# str }} delete_title, local_custom_matrix {{/ str }}" aria-label="Delete"></i>
                            </a>
                            {{/ matrixexist}}
                            {{# matrixexist}}
                            <a href="javascript:void(0)" title="{{# str}}delete{{/ str}}" onclick="(function(e){ require('local_custom_matrix/newcustommatrix').nodelete({selector: 'deletecustommatrix', name: '{{custom_matrix_name}}',skillid:{{custom_matrix_id}}, contextid:1, parentcatid:{{childs}}}) })(event)">
                                <i class="fa fa-trash fa-fw" aria-hidden="true" title="{{# str }} delete_title, local_custom_matrix {{/ str }}" aria-label="Delete"></i>
                            </a>
                            {{/ matrixexist}}
                          
                        </td>
                    </tr>
                {{/records}}
            </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}
