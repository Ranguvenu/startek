 <div class="col-md-12 col-sm-12 col-12 dept_btmcontent pull-left">
                {{^ hide_users}}
                <div class="w-full pull-left mt-3 dept_details dept_users">
                    <div class="pull-left col-lg-5 col-md-12 col-sm-12 col-12 px-2 deptdetails_left">
                        <span class="pull-left pr-2 userscolor"><i class="fa fa-user-plus" aria-hidden="true"></i></span>
                        <span class="details_string pull-left">{{# str}} users {{/str}}</span>
                        <span class="pull-right details_count userscolor"><a href="{{viewlink_url}}" class='' title= "{{#str}} totalusers, local_costcenter {{/str}}">{{ totalusers }}</a></span>
                    </div>
                    <div class="pull-left col-lg-7 col-md-12 col-sm-12 col-12 px-2 deptdetails_right">
                        <span class="w-half pull-left inforight"><i class="fa fa-user-o pr-1 pull-left activeicon" aria-hidden="true"></i>
                            <span class="lable_name pull-left">{{# str}} active {{/str}}<span class="label_colon pull-right mr-1">:</span></span><span class="userscolor"><a href="{{count_activelink_url}}" class='' title= "{{#str}} activeusers, local_costcenter {{/str}}">{{ activeusercount }}</a></span>
                        </span>

                         <span class="w-half pull-left"><i class="fa fa-user-times pr-1 pull-left inactiveicon" aria-hidden="true"></i><span class="lable_name pull-left">{{# str}} inactive {{/str}}<span class="label_colon pull-right mr-1">:</span></span><span class="userscolor"><a href="{{count_inactivelink_url}}" class='' title= "{{#str}} inactiveusers, local_costcenter {{/str}}">{{ inactiveusercount }}</a></span></span>
                    </div>
                </div>
                {{/ hide_users}}
