            <div class="d-flex align-items-center justify-content-between">
                <h3 class="org_name">{{ orgname }}</h3>
                <div class="dropdown action_menu_dropdown">
                    <span  data-toggle="dropdown" class="px-2" aria-expanded="false">
                        <i class="fa fa-ellipsis-h text-white"></i>
                    </span>
                    <div class="dropdown-menu dropdown-menu-right shadow-sm">
                        {{# edit}}
                        <a class="dropdown-item text-dark" href="javascript:void(0)" title="{{# str}} edit {{/str}}" onclick="(function(e){ require('local_costcenter/newcostcenter').init({ contextid:1, id:{{ recordid }}, formtype:'{{formtype}}', headstring:'{{headstring}}' }) })(event)"><i class="fa fa-pencil fa-fw"></i> {{# str}} edit {{/str}}</a>
                       
                        {{#profileinfocategory}}
                            <a class="dropdown-item text-dark" href='{{config.wwwroot}}/local/costcenter/manageuserfields.php?category={{profileinfocategory}}' title="{{# str}}userprofilefields,local_costcenter{{/ str}}"><i class="fa fa-users"></i>{{# str}}userprofilefields,local_costcenter{{/ str}}</a>
                        {{/profileinfocategory}}
                        
                        {{/ edit}}
                       
                       
                        <!--
                        {{#courseinfocategory}}
                            <a class="dropdown-item text-dark" href='{{config.wwwroot}}/local/costcenter/managecoursefields.php?category={{courseinfocategory}}' title="{{# str}}usercoursefields,local_costcenter{{/ str}}"><i class="fa fa-book"></i>{{# str}}usercoursefields,local_costcenter{{/ str}}</a>
                        {{/courseinfocategory}}
                       -->
                        {{# hide}}
                            <a class="dropdown-item text-dark" href="javascript:void(0)" title ="{{# str}} inactive {{/str}}" onclick = "(function(e){ require('local_costcenter/newcostcenter').costcenterStatus({action:'updatecostcenter', id: {{{recordid}}} ,confirm: {{{status}}} ,actionstatus:'Confirmation', actionstatusmsg:'{{{action_message}}}' }) })(event)"><i class="fa fa-eye" aria-hidden="true" aria-label="Inactive"></i>{{# str}} inactive {{/str}}</a>
                        {{/ hide}} 
                       {{# show}}
                            <a class="dropdown-item text-dark" href="javascript:void(0)" title ="{{# str}} active {{/str}}" onclick = "(function(e){ require('local_costcenter/newcostcenter').costcenterStatus({action:'updatecostcenter', id: {{{recordid}}} ,confirm: {{{status}}} ,actionstatus:'Confirmation', actionstatusmsg:'{{{action_message}}}' }) })(event)"><i class="fa fa-eye-slash" aria-hidden="true" aria-label="Active"></i>{{# str}} active {{/str}}</a>
                        {{/ show}}
                        
                        {{# delete}}
                        <a class="dropdown-item text-dark" href="javascript:void(0)" title ="{{# str}} delete {{/str}}" onclick = "(function(e){ require('local_costcenter/costcenterdatatables').costcenterDelete({action:'deletecostcenter', id: {{{ recordid }}} ,confirm: {{{status}}} ,actionstatus:'Confirmation', actionstatusmsg:'{{{ delete_message }}}' }) })(event)"><i class="fa fa-trash fa-fw " aria-hidden="true" aria-label="Delete"></i> Delete</a>
                        {{/ delete}}
                        
                    </div>
